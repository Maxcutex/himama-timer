<div class="col-md-6">
<%= form_with(model: time_entry, local: true) do |form| %>
  <% if time_entry.errors.any? %>
    <div id="error_explanation" class="alert alert-danger m-3">
      <p><%= pluralize(time_entry.errors.count, "error") %> prohibited this time_entry from being saved:</p>

      <ul>
        <% time_entry.errors.full_messages.each do |message| %>
          <li><%= message %></li>
        <% end %>
      </ul>

      
    </div>
  <% end %>
  <div class="form-group field">
  <%= form.label :starts_at, class: "h3" %><br>
  <%= form.datetime_field :starts_at,  class: "form-control mb-2 mr-sm-2 mb-sm-0" %>
</div>

<div class="form-group field">
  <%= form.label :finishes_at, class: "h3" %><br>
  <%= form.datetime_field :finishes_at,  class: "form-control mb-2 mr-sm-2 mb-sm-0" %>
</div>
 

  <div class="actions">
    
    <%= form.submit "Submit", class: "btn btn-default" %>
  </div>
<% end %>

</div>